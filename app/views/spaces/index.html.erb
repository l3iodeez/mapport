<h1>Listing spaces</h1>

<table>
  <thead>
    <tr>
      <th>Spacename</th>
      <th>Spaceid</th>
      <th>Roomnum</th>
      <th>Floor</th>
      <th>Roomsqft</th>
      <th>Building</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @spaces.each do |space| %>
      <tr>
        <td><%= space.spacename %></td>
        <td><%= space.spaceid %></td>
        <td><%= space.roomnum %></td>
        <td><%= space.floor %></td>
        <td><%= space.roomsqft %></td>
        <td><%= space.building %></td>
        <td><%= link_to 'Show', space %></td>
        <td><%= link_to 'Edit', edit_space_path(space) %></td>
        <td><%= link_to 'Destroy', space, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Space', new_space_path %>
